{
  "label": "ZoomInfo",
  "name": "zoominfo",
  "apiVersion": "v1",
  "internal": false,
  "configurationType": "GLOBAL",
  "deployment": {
    "profiles": [
      {
        "name": "default",
        "label": "Default",
        "memory": 256,
        "description": "Default configuration"
      }
    ],
    "allowMultipleInstances": false
  },
  "status": "ACTIVE",
  "stores": [
    {"name": "tokens"}
  ],
  "events": [
    {
      "label": "Webhook",
      "name": "webhook",
      "description": "Notifications from ZoomInfo about entities"
    }
  ],
  "functions": [
    {
      "label": "GET",
      "name": "_get"
    },
    {
      "label": "POST",
      "name": "_post"
    },
    {
      "label": "PUT",
      "name": "_put"
    }
  ],
  "scripts": [
    "zoominfo-fn-helpers.js",
    "http-generic-helpers.js"
  ],
  "configuration": [
    {
      "name": "userName",
      "label": "User Name",
      "type": "text",
      "required": true,
      "visibility": true
    }
  ]
}